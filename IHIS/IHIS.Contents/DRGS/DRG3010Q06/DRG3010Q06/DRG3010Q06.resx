  <data name="grdDrg3010.QuerySQL" xml:space="preserve">
    <value>SELECT FN_BAS_LOAD_HO_DONG_NAME(A.HO_DONG, A.ORDER_DATE) 
      ,FN_BAS_LOAD_GWA_NAME(A.GWA, A.JUBSU_DATE) BUSEO_NAME
      ,A.HO_CODE                        
      ,A.FKOCS2003             
      ,A.BUNHO                                        
      ,D.SUNAME                                     
      ,A.JAERYO_CODE                
      ,C.JAERYO_NAME        
      ,A.SUBUL_SURYANG            
      ,A.SUBUL_DANUI                
      ,A.ORD_SURYANG                
      ,FN_OCS_LOAD_CODE_NAME('ORD_DANUI', A.ORDER_DANUI) 
      ,A.DIVIDE             
      ,A.ORDER_DATE   
      ,A.DRG_BUNHO      
      ,DECODE(A.BUNRYU2,'1','Y','N')    
      ,A.MAGAM_SER                            
      ,NVL(A.BANNAB_YN,'N')                   
      ,DECODE(A.TOIWON_DRG_YN,'0','T',DECODE(A.EMERGENCY,'Y','E',DECODE(A.MAGAM_GUBUN,'21','A','N')))
      ,A.SOURCE_FKOCS2003 
  FROM OUT0101 D,
       INV0110 C,   
       INP1001 B,   
       DRG3010 A  
 WHERE A.ORDER_DATE        BETWEEN TO_DATE(:f_order_date_fr, 'YYYY/MM/DD')
                               AND TO_DATE(:f_order_date_to, 'YYYY/MM/DD')
   AND A.JAERYO_CODE       LIKE :f_jaeryo_code||'%'
   AND A.HO_DONG           LIKE :f_ho_dong||'%'
   AND A.BUNHO             LIKE :f_bunho||'%'
   AND A.BUNRYU1              = :f_bunryu1
   AND 'Y'                    = :f_magam_gubun
   AND A.DRG_BUNHO            &gt; 0
   AND ((NVL(A.DC_YN,'N')     = 'N' AND A.SOURCE_FKOCS2003 IS NULL AND A.CHULGO_DATE IS NOT NULL) 
    OR  (NVL(A.BANNAB_YN,'N') = 'Y' AND A.SOURCE_FKOCS2003 IS NOT NULL)                          
    OR  (NVL(A.BANNAB_YN,'N') = 'Y' AND A.SOURCE_FKOCS2003 IS NULL))                             
   AND B.PKINP1001            = A.FKINP1001        
   AND C.JAERYO_CODE          = A.JAERYO_CODE
   AND B.BUNHO = D.BUNHO
   AND B.BUNHO = A.BUNHO
UNION ALL
SELECT FN_BAS_LOAD_HO_DONG_NAME(A.HO_DONG, A.ORDER_DATE)
      ,FN_BAS_LOAD_GWA_NAME(A.GWA, A.JUBSU_DATE) BUSEO_NAME
      ,A.HO_CODE
      ,A.FKOCS2003
      ,A.BUNHO
      ,D.SUNAME
      ,A.JAERYO_CODE
      ,C.JAERYO_NAME
      ,A.SUBUL_SURYANG
      ,A.SUBUL_DANUI
      ,A.ORD_SURYANG
      ,FN_OCS_LOAD_CODE_NAME('ORD_DANUI', A.ORDER_DANUI)
      ,A.DIVIDE
      ,A.ORDER_DATE
      ,A.DRG_BUNHO
      ,DECODE(A.BUNRYU2,'1','Y','N')
      ,A.MAGAM_SER
      ,NVL(A.BANNAB_YN,'N')
      ,DECODE(A.TOIWON_DRG_YN,'0','T',DECODE(A.EMERGENCY,'Y','E',DECODE(A.MAGAM_GUBUN,'21','A','N')))
      ,A.SOURCE_FKOCS2003
  FROM OUT0101 D,
       INV0110 C,
       INP1001 B,
       DRG3010 A
 WHERE A.ORDER_DATE        BETWEEN TO_DATE(:f_order_date_fr, 'YYYY/MM/DD')
                               AND TO_DATE(:f_order_date_to, 'YYYY/MM/DD')
   AND A.JAERYO_CODE       LIKE :f_jaeryo_code||'%'
   AND A.HO_DONG           LIKE :f_ho_dong||'%'
   AND A.BUNHO             LIKE :f_bunho||'%'
   AND A.BUNRYU1              = :f_bunryu1
   AND A.DRG_BUNHO            IS  NULL 
   AND 'N'                    = :f_magam_gubun
   AND ((NVL(A.DC_YN,'N')     = 'N' AND A.SOURCE_FKOCS2003 IS NULL AND A.CHULGO_DATE IS NOT NULL)
    OR  (NVL(A.BANNAB_YN,'N') = 'Y' AND A.SOURCE_FKOCS2003 IS NOT NULL)
    OR  (NVL(A.BANNAB_YN,'N') = 'Y' AND A.SOURCE_FKOCS2003 IS NULL))
   AND B.PKINP1001            = A.FKINP1001
   AND C.JAERYO_CODE          = A.JAERYO_CODE
   AND B.BUNHO = D.BUNHO
   AND B.BUNHO = A.BUNHO
 ORDER BY 14, 4</value>
