  <data name="grdScPost_modifier.QuerySQL" xml:space="preserve">
    <value>SELECT A.SUSIK_CODE        ,
           A.SUSIK_NAME        ,
           A.SUSIK_NAME_GANA   ,
           A.SUSIK_DETAIL_GUBUN
      FROM CHT0115 A
     WHERE A.HOSP_CODE          = :f_hosp_code
       AND A.SUSIK_DETAIL_GUBUN = :f_susik_detail_gubun
       AND (    A.SUSIK_NAME      LIKE '%'||:f_susik_name||'%'
             OR A.SUSIK_NAME_GANA LIKE '%'||:f_susik_name||'%' )
       AND NVL(A.SUSIK_DISABLE_DATE, TO_DATE('99991231', 'YYYYMMDD')) &gt;= TRUNC(SYSDATE)
    ORDER BY A.SORT, A.SUSIK_NAME</value>
  </data>
  <data name="grdScPre_modifier.QuerySQL" xml:space="preserve">
    <value>    SELECT A.SUSIK_CODE        ,
           A.SUSIK_NAME        ,
           A.SUSIK_NAME_GANA   ,
           A.SUSIK_DETAIL_GUBUN
      FROM CHT0115 A
          ,VW_OUTSANG_SUSIK_RANK B
     WHERE A.HOSP_CODE          = :f_hosp_code
       AND A.SUSIK_DETAIL_GUBUN LIKE :f_susik_detail_gubun
       AND (    A.SUSIK_NAME      LIKE '%'||:f_susik_name||'%'
             OR A.SUSIK_NAME_GANA LIKE '%'||:f_susik_name||'%' )
       AND NVL(A.SUSIK_DISABLE_DATE, TO_DATE('99991231', 'YYYYMMDD')) &gt;= TRUNC(SYSDATE)
       --
       AND B.SUSIK_CODE (+) = A.SUSIK_CODE
       AND B.IO_GUBUN   (+) = :f_io_gubun
       AND B.DOCTOR     (+) = :f_user_id
     ORDER BY NVL(B.RANK_CNT, 0) DESC, A.SORT, A.SUSIK_CODE, A.SUSIK_NAME</value>
  </data>
