  <data name="grdNUR4002.QuerySQL" xml:space="preserve">
    <value>SELECT DISTINCT 
       A.PKNUR4002                                                                                     PKNUR4002,
       A.BUNHO                                                                                         BUNHO,
       A.FKINP1001                                                                                     FKINP1001,
       A.NUR_PLAN_JIN                                                                                  NUR_PLAN_JIN,
       A.NUR_PLAN_PRO                                                                                  NUR_PLAN_PRO,
       A.NUR_PLAN_PRO_NAME                                                                             NUR_PLAN_PRO_NAME,
       A.PLAN_USER                                                                                     PLAN_USER,
       FN_ADM_LOAD_USER_NM(A.PLAN_USER, TRUNC(A.PLAN_DATE))                                            PLAN_USER_NM,
       A.PLAN_DATE                                                                                     PLAN_DATE,
       NVL(A.VALD, 'N')                                                                                VALD,
       DECODE(C.FKNUR4002, NULL,'Y', 'N')                                                               MODIFY_YN,
       B.VALD                                                                                          VALD,
       A.SORT_KEY                                                                                      SORT_KEY,
       B.SORT_KEY                                                                                      SORT_KEY
  FROM NUR4002 A,
       NUR0402 B,
       NUR4005 C
 WHERE A.HOSP_CODE    = :f_hosp_code
   AND B.HOSP_CODE    = A.HOSP_CODE
   AND C.HOSP_CODE(+) = A.HOSP_CODE
   AND A.FKINP1001    = :f_fkinp1001
   --AND NVL(A.VALD, 'N')         = 'Y'
   AND B.NUR_PLAN_JIN = A.NUR_PLAN_JIN
   AND B.NUR_PLAN_PRO = A.NUR_PLAN_PRO
   AND C.FKNUR4002(+) = A.PKNUR4002
 ORDER BY TO_CHAR(A.PKNUR4002, '000000000000000')||
          TO_CHAR(A.PLAN_DATE, 'YYYYMMDD')||
          LTRIM(NVL(A.SORT_KEY, '00'))||
          LTRIM(NVL(B.SORT_KEY, '00'))</value>
  </data>
