  <data name="fwkAssessor.InputSQL" xml:space="preserve">
    <value>SELECT USER_ID, USER_NM 
  FROM ADM3200 
 WHERE HOSP_CODE = :f_hosp_code
   AND DEPT_CODE = (SELECT B.BUSEO_CODE
                      FROM BAS0260 B
                      WHERE B.BUSEO_NAME = :f_buseo_name)
 ORDER BY USER_ID

/*
SELECT A.USER_ID, A.USER_NM
  FROM ADM3211 A, ADM3200 C
 WHERE A.HOSP_CODE  = FN_ADM_LOAD_HOSP_CODE
   AND C.HOSP_CODE  = A.HOSP_CODE
   AND A.START_DATE = (SELECT MAX(B.START_DATE)
                         FROM ADM3211 B
                        WHERE B.HOSP_CODE = A.HOSP_CODE
                          AND B.USER_ID   = A.USER_ID
                          AND TRUNC(SYSDATE) BETWEEN B.START_DATE AND NVL(B.END_DATE, TO_DATE('9999/12/31', 'YYYY/MM/DD')))
     AND A.USER_ID = C.USER_ID
     AND C.USER_GUBUN = '2'
 ORDER BY USER_ID
*/</value>
  </data>
