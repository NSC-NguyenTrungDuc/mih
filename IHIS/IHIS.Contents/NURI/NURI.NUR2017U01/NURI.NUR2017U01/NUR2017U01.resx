  <data name="grdNUR2017.QuerySQL" xml:space="preserve">
    <value>SELECT E.HO_CODE1                   
     , C.BUNHO                      
     , E.SUNAME                     
     , FN_BAS_LOAD_GWA_NAME(E.GWA, B.ORDER_DATE)                    GWA_NAME 
     , B.HANGMOG_CODE               
     , A.HANGMOG_NAME   
     , B.ACT_RES_DATE                            ACT_RES_DATE
     , DECODE(SUBSTR(NVL(B.ACTING_SURYANG,0),1,1),'.','0'||ROUND(B.ACTING_SURYANG,3) , ROUND(B.ACTING_SURYANG,3)) SURYANG       
     , C.ORD_DANUI                  
     , C.DV_TIME                    
     , 1                                                            DV 
     , FN_OCS_LOAD_JUSA_CUT_NAME(C.JUSA)                       JUSA
     , FN_OCS_BOGYONG_COL_NAME(A.ORDER_GUBUN, C.BOGYONG_CODE, C.JUSA_SPD_GUBUN) BOGYONG_CODE
     , NVL(B.ACTING_DATE, B.PASS_DATE)                                   ACTING_DATE
     , NVL(DECODE(B.ACTING_DATE,  NULL, '', NVL(B.ACTING_TIME, '0000')) 
          ,DECODE(B.PASS_DATE,  NULL, '', NVL(B.PASS_TIME, '0000')))     ACTING_TIME
     , NVL(B.ACT_USER, B.PASS_USER)                                      ACTING_USER
     , DECODE(B.ACTING_DATE,  NULL,'N','Y')                              ACTING_YN
     , B.FKOCS2003
     , B.SEQ
     , B.ORDER_DATE                                                                
     , NVL(F.BOGYONG_NAME,' ')                                      BOGYONG_NAME 
     , C.DC_YN                                                                      
     , SUBSTR(C.GROUP_SER, 2 )                                     GROUP_SER 
     , C.MIX_GROUP                                                                  
     , C.HOPE_DATE                                                                  
     , FN_OCSI_LOAD_OCS0132_CODE_NAME('JUSA', C.JUSA)               JUSA_TEXT                     
     , FN_OCSI_LOAD_OCS0132_CODE_NAME('ORD_DANUI', C.ORD_DANUI)     DANUI_TEXT          
     , C.ORDER_REMARK                                                               
     , C.NURSE_REMARK                                                               
     , FN_OCSI_LOAD_OCS0132_CODE_NAME('INPUT_GUBUN', C.INPUT_GUBUN) INPUT_GUBUN_TEXT
     , C.JUSA_SPD_GUBUN                                             JUSA_SPD_GUBUN  
     , FN_BAS_LOAD_SABUN_NAME(NVL(B.ACT_USER, B.PASS_USER))                           ACT_USER_NAME   
     , B.BANNAB_YN                                                  BANNAB_YN       
     , B.BANNAB_FKOCS2003                                           BANNAB_FKOCS2003
     , NVL(B.BANNAB_YN, 'N')                                        OLD_BANNAB_YN  
     , B.BANNAB_YN                                                  DRUG_BANNAB_YN
     , CASE WHEN NVL(C.DC_GUBUN, 'N') = 'Y' AND NVL(C.DC_YN, 'N') = 'Y' THEN 'Y'
            ELSE 'N'
       END                                                          ORDER_DC_YN
     , CASE WHEN NVL(C.DC_GUBUN, 'N') = 'Y' AND NVL(C.DC_YN, 'N') = 'N' THEN 'Y'
            ELSE 'N'
       END                                                          ORDER_BANNAB_YN 
     , CASE WHEN C.ACTING_DATE IS NOT NULL THEN 'Y'
                                           ELSE 'N'
       END                                   BANNAB_ACTING_YN
     , FN_BAS_LOAD_CODE_NAME('DV_PATTERN_GUBUN', DECODE(B.SEQ, '0', '0', INSTR(SUBSTR(F.PATTERN,2), '1', 1, B.SEQ))) AS PATTERN_GUBUN
     , C.ORDER_GUBUN 
     , FN_OCS_LOAD_CODE_NAME('ORDER_GUBUN_BAS', SUBSTR(C.ORDER_GUBUN,2,1)) ORDER_GUBUN_NAME
     , DECODE(B.PASS_DATE,  NULL,'N','Y')                                PASS_YN
     , NVL(B.IF_DATA_SEND_YN, 'N')                                       IF_DATA_SEND_YN     
  FROM OCS0103        A,
       OCS2017        B,
       OCS2003        C,
       DRG0120        F, /* 복용법 */
       VW_OCS_INP1001 E
 WHERE A.HOSP_CODE       = :f_hosp_code
   AND B.HOSP_CODE       = A.HOSP_CODE
   AND C.HOSP_CODE       = A.HOSP_CODE
   AND E.HOSP_CODE       = A.HOSP_CODE
   AND F.HOSP_CODE(+)    = C.HOSP_CODE
   AND C.NDAY_YN         = 'N'
   AND C.FKINP1001       = E.PKINP1001
   AND A.HANGMOG_CODE    = C.HANGMOG_CODE
   AND C.PKOCS2003       = B.FKOCS2003
   AND NVL(C.PRN_YN,'N') = 'N'
   AND (C.INPUT_GUBUN LIKE 'D%' OR C.INPUT_GUBUN = 'NR')
   AND ((NVL(:f_order_gubun, 'A')  = 'A' AND B.DISPLAY_SEQ IN( 3, 4, 5)) -- 병상관리서 오픈시
    OR  (NVL(:f_order_gubun, 'A')  = 'C' AND B.DISPLAY_SEQ = 5)      -- 내복약
    OR  (NVL(:f_order_gubun, 'A') != 'C' AND B.DISPLAY_SEQ IN( 3, 4))) -- 주사,수액
   AND B.ACT_RES_DATE   = TO_DATE(:f_act_res_date, 'YYYY/MM/DD')   
   AND ( (C.ORDER_GUBUN IN ('0C','0D') AND  NVL(B.CANCEL_YN,'N') = 'Y' 
        AND  TO_DATE(:f_act_res_date, 'YYYY/MM/DD')BETWEEN TO_CHAR             (FN_OCSI_LOAD_BANNAB_FROM_DATE(C.PKOCS2003), 'YYYY/MM/DD') AND 
             TO_CHAR(FN_OCSI_LOAD_BANNAB_TO_DATE(C.PKOCS2003), 'YYYY/MM/DD'))
            OR 
             (C.ORDER_GUBUN IN ('0B') AND  NVL(B.CANCEL_YN,'N') = 'Y')
             OR
             (NVL(B.CANCEL_YN,'N') = 'N') 
            )
   AND E.BUNHO          = :f_bunho
   AND NVL(C.ERROR_FLAG, 'N') = 'N'
   AND C.OCS_FLAG &gt; '0'
   AND NVL(C.TOIWON_DRG_YN, 'N') = 'N'
   AND C.BOGYONG_CODE = F.BOGYONG_CODE(+)
   AND C.ACTING_DATE IS NOT NULL
   AND ( (:f_bannab_del = 'Y' AND NVL(C.BANNAB_YN,'N') ='N')
         OR :f_bannab_del = 'N') 
ORDER BY SUBSTR(C.ORDER_GUBUN, 2) DESC --薬優先
       , DECODE(B.SEQ, '0', '0', INSTR(SUBSTR(F.PATTERN,2), '1', 1, B.SEQ)) --食事順
       , B.SEQ -- 回数の何番名か。
       , C.GROUP_SER -- グループ番号順
       , C.MIX_GROUP --ミックス順
       , C.PKOCS2003</value>
