  <data name="layCalendar.QuerySQL" xml:space="preserve">
    <value>SELECT BUNHO
     , TO_CHAR(FROM_DATE,'YYYY/MM/DD')
     , BEDSORE_BUWI
     , TO_CHAR(ASSESSOR_DATE,'YYYY/MM/DD')
     , ASSESSOR
  FROM NUR6002 A
 WHERE A.HOSP_CODE = :f_hosp_code
   AND A.BUNHO     = :f_bunho
   AND A.FROM_DATE = TO_DATE(:f_from_date, 'YYYY/MM/DD')
   AND A.ASSESSOR_DATE BETWEEN TO_DATE(:f_from_month||'01','YYYYMMDD')
                           AND LAST_DAY(TO_DATE(:f_from_month||'01','YYYYMMDD'))
 ORDER BY A.ASSESSOR_DATE, A.BEDSORE_BUWI</value>
