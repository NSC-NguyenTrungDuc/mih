  <data name="grdDrg3010.QuerySQL" xml:space="preserve">
    <value>SELECT A.FKOCS2003
     , A.BUNHO
     , B.SUNAME
     , A.ORDER_DATE
     , A.DRG_BUNHO
     , FN_BAS_LOAD_HO_DONG_NAME(A.HO_DONG, A.ORDER_DATE) HO_DONG
     , A.HO_CODE 
     , FN_BAS_LOAD_DOCTOR_NAME(A.DOCTOR, A.ORDER_DATE)
     , FN_BAS_LOAD_GWA_NAME(A.GWA, TRUNC(SYSDATE))
     , A.JAERYO_CODE
     , A.NALSU
     , A.DIVIDE
     , A.ORDER_SURYANG
     , A.SUBUL_SURYANG
     , A.ORD_SURYANG
     , FN_OCS_LOAD_CODE_NAME('ORD_DANUI',A.ORDER_DANUI) 
     , FN_OCS_LOAD_CODE_NAME('ORD_DANUI',A.SUBUL_DANUI) 
     , FN_OCS_LOAD_BOGYONG_NAME(A.BOGYONG_CODE)
     , A.DC_YN
     , A.DV_TIME
     , A.DV 
     , C.JAERYO_NAME
  FROM INV0110 C
     , OUT0101 B
     , DRG3010 A
 WHERE A.ORDER_DATE    BETWEEN TO_DATE(:f_jubsu_date, 'YYYY/MM/DD') AND TO_DATE(:f_jubsu_date_to, 'YYYY/MM/DD')
   AND A.BUNHO               LIKE :f_bunho||'%'
   AND A.HO_DONG             LIKE :f_ho_dong||'%'
   AND NVL(A.TOIWON_DRG_YN,'N') &lt;&gt; 'N'
   AND A.BUNHO              = B.BUNHO(+)
   AND C.JAERYO_GUBUN       = 'A'
   AND C.JAERYO_CODE        LIKE :f_jaeryo_code||'%'
   AND A.JAERYO_CODE    = C.JAERYO_CODE
   AND NVL(A.DC_YN,'N')     = 'N'
   AND A.SOURCE_FKOCS2003   IS NULL
   AND  ((:f_gubun = '1' AND A.CHULGO_DATE IS NOT NULL )
    OR   (:f_gubun = '2' AND A.CHULGO_DATE IS NULL) )
 ORDER BY A.ORDER_DATE, A.FKOCS2003</value>
  </data>
  <data name="cboDong.UserSQL" xml:space="preserve">
    <value>SELECT A.BUSEO_CODE, A.BUSEO_NAME
  FROM VW_GWA_NAME A
 WHERE A.START_DATE = ( SELECT MAX(Z.START_DATE)
                         FROM VW_GWA_NAME Z
                        WHERE Z.BUSEO_CODE  = A.BUSEO_CODE
                          AND Z.BUSEO_GUBUN = A.BUSEO_GUBUN
                          AND Z.START_DATE  &lt;= TRUNC(SYSDATE) )
   AND A.BUSEO_GUBUN = '2' 
UNION ALL
SELECT 'EM',   '救急室'
  FROM DUAL
UNION ALL
SELECT 'OPAN', '麻酔準備室'
  FROM DUAL
UNION ALL
SELECT '%', '全体'
  FROM DUAL
ORDER BY 1</value>
