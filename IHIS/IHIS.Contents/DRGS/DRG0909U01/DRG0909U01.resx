  <data name="grdDRG0909.QuerySQL" xml:space="preserve">
    <value>SELECT D.HANGMOG_CODE ,
       D.HANGMOG_NAME,
       FN_OCS_LOAD_CODE_NAME('ORD_DANUI',  D.ORD_DANUI,   A.HOSP_CODE )  ORDER_DANUI,
       FN_OCS_LOAD_CODE_NAME('ORD_DANUI',  E.DANWI,       A.HOSP_CODE )  SUNAB_DANUI,
       FN_OCS_LOAD_CODE_NAME('ORD_DANUI',  F.SUBUL_DANUI, A.HOSP_CODE )  SUBUL_DANUI,
       A.SEQ  ,
       A.ORD_DANUI    ,
       FN_OCS_LOAD_CODE_NAME('ORD_DANUI',  A.ORD_DANUI, A.HOSP_CODE ) DANUI  ,
       NVL(A.CHANGE_QTY1, 0),
       NVL(A.CHANGE_QTY2, 0),
       F.JAERYO_CODE,
       F.JAERYO_NAME,
       NVL(FN_INV_LOAD_CODE_NAME('UA', F.BUNRYU1, NULL, A.HOSP_CODE ), ' '),
       NVL(substrb(FN_DRG_LOAD_DRG0142(F.SMALL_CODE, NULL, A.HOSP_CODE ),1,100), ' '),
       NVL(substrb(FN_DRG_LOAD_DRG0142(F.SMALL_CODE1, NULL, A.HOSP_CODE),1,100), ' '),
       E.SG_CODE,
       E.SG_NAME,
       E.SG_UNION,
       DECODE(NVL(TO_CHAR(F.BULYONG_DATE,'yyyy'),'N'), 'N', 'Y','N') 
  FROM OCS0108 A,
       OCS0103 D,
       BAS0310 E,
       INV0110 F
 WHERE A.HOSP_CODE      = :f_hosp_code
   AND A.HANGMOG_CODE   = D.HANGMOG_CODE
   AND D.SG_CODE        = E.SG_CODE(+)
   AND E.BULYONG_YMD   IS NULL
   AND (D.JUNDAL_TABLE_OUT = :f_query OR D.JUNDAL_TABLE_INP = :f_query)
   AND :f_query = 'DRG' 
   AND F.JAERYO_CODE (+)= D.HANGMOG_CODE
   AND A.HOSP_CODE      = D.HOSP_CODE
   AND A.HOSP_CODE      = E.HOSP_CODE
   AND A.HOSP_CODE      = F.HOSP_CODE
 UNION ALL 
SELECT DISTINCT D.HANGMOG_CODE ,
       D.HANGMOG_NAME,
       NULL ORDER_DANUI,
       NULL SUNAB_DANUI,
       NULL SUBUL_DANUI,
       NULL SEQ  ,
       NULL ORD_DANUI    ,
       NULL DANUI  ,
       NULL CHANGE_QTY1,
       NULL CHANGE_QTY2,
       F.HANGMOG_CODE,
       F.GUMSA_NAME,
       NULL,
       NULL,
       NULL,
       E.SG_CODE,
       E.SG_NAME,
       E.SG_UNION,
       ''
 FROM  OCS0103 D,
       BAS0310 E,
       CPL0101 F
WHERE D.HOSP_CODE     = :f_hosp_code
  AND D.SG_CODE       = E.SG_CODE (+)
  AND D.HANGMOG_CODE  = F.HANGMOG_CODE(+)
  AND D.HOSP_CODE     = E.HOSP_CODE(+)
  AND D.HOSP_CODE     = F.HOSP_CODE(+)
  AND F.EMERGENCY = 'N'
  AND (D.JUNDAL_TABLE_OUT = :f_query OR D.JUNDAL_TABLE_INP = :f_query)
  AND :f_query = 'CPL'
ORDER BY 1</value>
