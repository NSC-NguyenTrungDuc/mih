  <data name="layBeopoListDetail.QuerySQL" xml:space="preserve">
    <value>SELECT B.BUSEO_NAME
     , 'Y'
     , B.BUSEO_CODE 
     , NULL 
     , B.PARENT_BUSEO 
     , FN_BAS_LOAD_BUSEO_NAME(B.PARENT_BUSEO, SYSDATE) 
  FROM BAS0260                    B
     , ADM0750                    A
 WHERE A.HOSP_CODE                = :f_hosp_code
   AND A.USER_ID                  = :f_user_id
   AND A.SEQ                      = :f_seq
   AND A.BEOPO_GUBUN              = 'B'
   --
   AND B.HOSP_CODE                = A.HOSP_CODE
   AND B.BUSEO_CODE               = A.RECV_SPOT
   AND B.START_DATE               = (SELECT MAX(Z.START_DATE)
                                       FROM BAS0260          Z
                                      WHERE Z.HOSP_CODE      = A.HOSP_CODE
                                        AND Z.BUSEO_CODE     = A.RECV_SPOT
                                        AND (   Z.END_DATE   IS NULL
                                             OR Z.END_DATE   &gt;= TRUNC(SYSDATE)
                                            )
                                    )
   AND (   B.END_DATE             IS NULL 
        OR B.END_DATE             &gt;= TRUNC(SYSDATE)
       )
UNION ALL
SELECT B.USER_NM
     , 'N'
     , NULL
     , B.USER_ID 
     , B.DEPT_CODE 
     , FN_BAS_LOAD_BUSEO_NAME(B.DEPT_CODE, SYSDATE)
  FROM ADM3200 B
     , ADM0750 A
