  <data name="lay9001R.QuerySQL" xml:space="preserve">
    <value>SELECT DISTINCT
       TO_CHAR(E.HOLI_DAY, 'YYYY/MM')                  AS HOLI_DAY
     , NVL(A.SU, 0)                                    AS DRG_CNT
     , NVL(G.SU, 0)                                    AS DRG_GROUP_CNT
     , NVL(C.SU, 0)                                    AS DRG_HANGMOG_CNT
     , NVL(B.SU, 0)                                    AS INJ_CNT
     , NVL(H.SU, 0)                                    AS INJ_GROUP_CNT
     , NVL(D.SU, 0)                                    AS INJ_HANGMOG_CNT
     , NVL(A.SU, 0) + NVL(B.SU, 0)                     AS DRG_INJ_CNT
     , NVL(G.SU, 0) + NVL(H.SU, 0)                     AS DRG_INJ_GROUP_CNT
     , NVL(C.SU, 0) + NVL(D.SU, 0)                     AS DRG_INJ_HANGMOG_CNT
  FROM (SELECT NAEWON_DATE , COUNT(*)  SU
          FROM (
                SELECT TO_CHAR(X.NAEWON_DATE, 'YYYY/MM') NAEWON_DATE, X.DRG_BUNHO, SUM(1)
                  FROM DRG2010 X
                 WHERE X.HOSP_CODE = :f_hosp_code
                   AND X.NAEWON_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND X.BUNRYU1   &lt;&gt; '4'
                   AND X.ACT_DATE  IS NOT NULL
                   AND NVL(X.DC_YN, 'N')     = 'N'
                   AND NVL(X.BANNAB_YN, 'N') = 'N'
                   AND X.BUNHO               NOT LIKE '09%'
                 GROUP BY X.NAEWON_DATE, X.DRG_BUNHO
                )
         GROUP BY NAEWON_DATE ) A,
       (SELECT NAEWON_DATE , COUNT(CNT)  SU
          FROM (
                SELECT TO_CHAR(Z.JUBSU_DATE, 'YYYY/MM') NAEWON_DATE, COUNT(1) CNT
                  FROM DRG2010 X
                     , OCS1003 Y
                     , DRG2030 Z
                 WHERE X.HOSP_CODE = :f_hosp_code
                   AND X.NAEWON_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND X.BUNRYU1   &lt;&gt; '4'
                   AND X.ACT_DATE  IS NOT NULL
                   AND X.BUNHO     NOT LIKE '09%'
                   AND NVL(X.DC_YN, 'N')     = 'N'
                   AND NVL(X.BANNAB_YN, 'N') = 'N'
                   --
                   AND Y.HOSP_CODE = X.HOSP_CODE
                   AND Y.PKOCS1003 = X.FKOCS1003
                   --
                   AND Z.HOSP_CODE = Y.HOSP_CODE
                   AND Z.FKOCS1003 = Y.PKOCS1003
                 GROUP BY Z.JUBSU_DATE, Z.DRG_BUNHO, Z.SERIAL_V
               )
         GROUP BY NAEWON_DATE ) G,
       (SELECT NAEWON_DATE , SUM(CNT)  SU
          FROM (
                SELECT TO_CHAR(X.NAEWON_DATE, 'YYYY/MM') NAEWON_DATE, X.DRG_BUNHO, COUNT(X.JAERYO_CODE) CNT
                  FROM DRG2010 X
                 WHERE X.HOSP_CODE = :f_hosp_code
                   AND X.NAEWON_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND X.BUNRYU1   &lt;&gt; '4'
                   AND X.ACT_DATE  IS NOT NULL
                   AND X.BUNHO     NOT LIKE '09%'
                   AND NVL(X.DC_YN, 'N')     = 'N'
                   AND NVL(X.BANNAB_YN, 'N') = 'N'
                 GROUP BY X.NAEWON_DATE, X.DRG_BUNHO
               )
         GROUP BY NAEWON_DATE ) C,
       (SELECT ACTING_DATE NAEWON_DATE, COUNT(*)  SU
          FROM (SELECT TO_CHAR(Z.ACTING_DATE, 'YYYY/MM') ACTING_DATE, Z.BUNHO, SUM(1)
                  FROM OCS1003 Z
                 WHERE Z.HOSP_CODE   = :f_hosp_code
                   AND Z.SLIP_CODE   LIKE 'J%'
                   AND Z.ACTING_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND Z.ACTING_DATE IS NOT NULL
                   AND Z.BUNHO       NOT LIKE '09%'
                   AND NVL(Z.DC_YN, 'N')     = 'N'
                   AND NVL(Z.BANNAB_YN, 'N') = 'N'
                 GROUP BY Z.ACTING_DATE, Z.BUNHO)
        GROUP BY ACTING_DATE
              ) B,
       (SELECT ACTING_DATE NAEWON_DATE, COUNT(CNT)  SU
          FROM (
                SELECT TO_CHAR(Z.ACTING_DATE, 'YYYY/MM') ACTING_DATE, Z.BUNHO, SUM(1) CNT
                  FROM OCS1003 Z
                 WHERE Z.HOSP_CODE   = :f_hosp_code
                   AND Z.SLIP_CODE   LIKE 'J%'
                   AND Z.ACTING_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND Z.ACTING_DATE IS NOT NULL
                   AND Z.BUNHO       NOT LIKE '09%'
                   AND NVL(Z.DC_YN, 'N')     = 'N'
                   AND NVL(Z.BANNAB_YN, 'N') = 'N'
                 GROUP BY Z.ACTING_DATE, Z.BUNHO, Z.GROUP_SER
                )
          GROUP BY ACTING_DATE ) H,
       (SELECT ACTING_DATE NAEWON_DATE, SUM(CNT)  SU
          FROM (SELECT TO_CHAR(Z.ACTING_DATE, 'YYYY/MM') ACTING_DATE, Z.BUNHO, COUNT(Z.HANGMOG_CODE) CNT
                  FROM OCS1003 Z
                 WHERE Z.HOSP_CODE   = :f_hosp_code
                   AND Z.SLIP_CODE   LIKE 'J%'
                   AND Z.ACTING_DATE BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') 
                                         AND TO_DATE(:f_date||'1231','YYYYMMDD')
                   AND Z.ACTING_DATE IS NOT NULL
                   AND Z.BUNHO       NOT LIKE '09%'
                   AND NVL(Z.DC_YN, 'N')     = 'N'
                   AND NVL(Z.BANNAB_YN, 'N') = 'N'
                 GROUP BY Z.ACTING_DATE, Z.BUNHO
                )
          GROUP BY ACTING_DATE ) D,
       RES0101 E
 WHERE E.HOSP_CODE  = :f_hosp_code
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = A.NAEWON_DATE(+)
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = B.NAEWON_DATE(+)
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = C.NAEWON_DATE(+)
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = D.NAEWON_DATE(+)
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = G.NAEWON_DATE(+)
   AND TO_CHAR(E.HOLI_DAY, 'YYYY/MM')   = H.NAEWON_DATE(+)
   AND E.HOLI_DAY   BETWEEN TO_DATE(:f_date||'01','YYYYMM') AND LAST_DAY(TO_DATE(:f_date||'12','YYYYMM'))
 ORDER BY 1</value>
  </data>
