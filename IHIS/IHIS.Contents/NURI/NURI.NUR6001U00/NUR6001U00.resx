  <data name="grdNur6002Copy.QuerySQL" xml:space="preserve">
    <value>SELECT A.BUNHO
     , TO_CHAR(A.FROM_DATE,'YYYY/MM/DD')
     , A.BEDSORE_BUWI
     , TO_CHAR(A.ASSESSOR_DATE,'YYYY/MM/DD')
     , A.ASSESSOR
     , A.BEDSORE_DEEP
