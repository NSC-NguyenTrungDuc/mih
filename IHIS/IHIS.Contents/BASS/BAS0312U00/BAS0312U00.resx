  <data name="layGroup.QuerySQL" xml:space="preserve">
    <value>SELECT B.SG_YMD        SG_YMD
     , B.SEQ           SEQ
     , B.SUB_SG_CODE   SUB_SG_CODE
     , A.SG_NAME       SG_NAME     
     , NVL(B.FROM_MONTH, 0)    FROM_MONTH
     , NVL(B.TO_MONTH, 0)      TO_MONTH
     , NVL(B.SURYANG, 0)       SURYANG
     , NVL(B.CAR_SURYANG, 0)   CAR_SURYANG
     , B.SG_CODE               SG_CODE
     , B.SEQ                   OLD_SEQ
     , NVL(B.SURYANG_YN, 'N')  SURYANG_YN
  FROM BAS0312 B,
       (SELECT 
               Z.SG_CODE
             , Z.SG_NAME
          FROM BAS0310 Z
         WHERE Z.HOSP_CODE          = :f_hosp_code 
           AND Z.SG_YMD             = (SELECT MAX(X.SG_YMD)
                                         FROM BAS0310 X
                                        WHERE X.HOSP_CODE       = :f_hosp_code
                                          AND X.SG_CODE         = Z.SG_CODE 
                                          AND X.SG_YMD          &gt;= :f_sg_ymd
                                          AND (   X.BULYONG_YMD &gt;= TRUNC(SYSDATE)
                                               OR X.BULYONG_YMD IS NULL
                                              )
                                      )
           AND (   Z.BULYONG_YMD  &gt;= :f_sg_ymd
                OR Z.BULYONG_YMD IS NULL
               )
       ) A
 WHERE B.HOSP_CODE      = :f_hosp_code 
      AND B.SG_CODE     = :f_sg_code 
      AND B.SG_YMD      = :f_sg_ymd
      --
      AND A.SG_CODE           (+)= B.SUB_SG_CODE       
 ORDER BY B.SEQ,B.FROM_MONTH

/*                  
SELECT B.SG_YMD        SG_YMD
     , B.SEQ           SEQ
     , B.SUB_SG_CODE   SUB_SG_CODE
     , A.SG_NAME       SG_NAME     
     , NVL(B.FROM_MONTH, 0)    FROM_MONTH
     , NVL(B.TO_MONTH, 0)      TO_MONTH
     , NVL(B.SURYANG, 0)       SURYANG
     , NVL(B.CAR_SURYANG, 0)   CAR_SURYANG
     , B.SG_CODE               SG_CODE
     , B.SEQ                   OLD_SEQ
  FROM BAS0312 B,
       BAS0310 A
 WHERE B.HOSP_CODE   = :f_hosp_code
   AND B.SG_CODE     = :f_sg_code
   AND B.SG_YMD      = :f_sg_ymd
   AND B.SUB_SG_CODE = A.SG_CODE(+)
   AND A.SG_YMD      = ( SELECT MAX(Z.SG_YMD) FROM BAS0310 Z
                          WHERE Z.HOSP_CODE   = :f_hosp_code
                            AND Z.SG_CODE = B.SUB_SG_CODE
                            AND B.SG_YMD BETWEEN Z.SG_YMD AND NVl(Z.BULYONG_YMD, '9998/12/31')) 
 ORDER BY B.SEQ,B.FROM_MONTH
*/</value>
