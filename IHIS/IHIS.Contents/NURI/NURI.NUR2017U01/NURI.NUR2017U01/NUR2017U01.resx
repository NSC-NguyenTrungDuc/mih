  <data name="grdPalist.QuerySQL" xml:space="preserve">
    <value>SELECT DISTINCT
        E.HO_DONG1,
        E.HO_CODE1,
        E.BED_NO,
        E.BUNHO, 
        E.SUNAME,
        E.PKINP1001,
        FN_BAS_LOAD_AGE('2013/09/20',E.BIRTH,'')||'/'||E.SEX                   AGE_SEX,
        E.IPWON_DATE                                                      IPWON_DATE,
        FN_BAS_LOAD_DOCTOR_NAME(E.DOCTOR, TO_DATE('2013/09/20', 'YYYY/MM/DD')) DOCTOR_NAME,
        D.HO_SORT
  FROM OCS0103       A,
       OCS2017        B,
       OCS2003        C,
       BAS0250        D,
       DRG0120        F,
       VW_OCS_INP1001 E
 WHERE E.HOSP_CODE       = :f_hosp_code
   AND E.HO_DONG1        = :f_ho_dong
   AND D.HOSP_CODE       = E.HOSP_CODE
   AND C.HOSP_CODE       = E.HOSP_CODE   
   AND F.HOSP_CODE(+)    = C.HOSP_CODE 
   AND B.HOSP_CODE       = E.HOSP_CODE
   AND A.HOSP_CODE       = E.HOSP_CODE   
   AND C.NDAY_YN         = 'N'
   AND C.FKINP1001       = E.PKINP1001
   AND C.HANGMOG_CODE    = A.HANGMOG_CODE
   AND C.PKOCS2003       = B.FKOCS2003
   AND D.HO_DONG         = E.HO_DONG1
   AND D.HO_CODE         = E.HO_CODE1
   AND NVL(C.PRN_YN,'N') = 'N'
   AND (C.INPUT_GUBUN LIKE 'D%' OR C.INPUT_GUBUN = 'NR')
   AND ((NVL(:f_order_gubun, 'A')  = 'A' AND B.DISPLAY_SEQ IN( 3, 4, 5)) 
    OR  (NVL(:f_order_gubun, 'A')  = 'C' AND B.DISPLAY_SEQ = 5) 
    OR  (NVL(:f_order_gubun, 'A') != 'C' AND B.DISPLAY_SEQ IN( 3, 4)))
   AND B.ACT_RES_DATE   = TO_DATE(:f_act_res_date, 'YYYY/MM/DD')   
   AND ( (C.ORDER_GUBUN IN ('0C','0D') AND  NVL(B.CANCEL_YN,'N') = 'Y' 
        AND  TO_DATE(:f_act_res_date, 'YYYY/MM/DD')BETWEEN TO_CHAR             (FN_OCSI_LOAD_BANNAB_FROM_DATE(C.PKOCS2003), 'YYYY/MM/DD') AND 
             TO_CHAR(FN_OCSI_LOAD_BANNAB_TO_DATE(C.PKOCS2003), 'YYYY/MM/DD'))
            OR 
             (C.ORDER_GUBUN IN ('0B') AND  NVL(B.CANCEL_YN,'N') = 'Y')
             OR
             (NVL(B.CANCEL_YN,'N') = 'N') 
            )
   AND NVL(C.ERROR_FLAG, 'N') = 'N'
   AND C.OCS_FLAG &gt; '0'
   AND NVL(C.TOIWON_DRG_YN, 'N') = 'N'
   AND C.BOGYONG_CODE = F.BOGYONG_CODE(+)
   AND C.ACTING_DATE IS NOT NULL
   AND ((NVL(:f_a, 'N') = 'Y' and D.TEAM = 'A') OR
        (NVL(:f_b, 'N') = 'Y' and D.TEAM = 'B') OR
        (NVL(:f_c, 'N') = 'Y' and D.TEAM = 'C') OR
        (NVL(:f_d, 'N') = 'Y' and D.TEAM = 'D') OR        
        (NVL(:f_a, 'N') = 'N' AND 
         NVL(:f_b, 'N') = 'N' AND
         NVL(:f_c, 'N') = 'N' AND
         NVL(:f_d, 'N') = 'N' ))

 ORDER BY D.HO_SORT, E.BED_NO</value>
  </data>
