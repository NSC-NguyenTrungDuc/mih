  <data name="lay9004R.QuerySQL" xml:space="preserve">
    <value>SELECT TO_CHAR(A.ACTING_DATE, 'YYYY/MM') ACTING_MONTH
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'CR', NVL(B.XRAY_REAL_CNT, 1)))), 0) CR_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'CR', NVL(B.XRAY_REAL_CNT, 1)))), 0) CR_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'DR', NVL(B.XRAY_REAL_CNT, 1)))), 0) DR_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'DR', NVL(B.XRAY_REAL_CNT, 1)))), 0) DR_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'CT', NVL(B.XRAY_REAL_CNT, 1)))), 0) CT_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'CT', NVL(B.XRAY_REAL_CNT, 1)))), 0) CT_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'MRI', NVL(B.XRAY_REAL_CNT, 1)))), 0) MRI_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'MRI', NVL(B.XRAY_REAL_CNT, 1)))), 0) MRI_CNT_Y
     , SUM(NVL(B.XRAY_REAL_CNT, 1)) TOTAL_CNT
     , '0'
  FROM XRT0001 B
     , OCS2003 A
 WHERE A.HOSP_CODE    = :f_hosp_code
   AND A.JUNDAL_TABLE = 'XRT'
   AND A.JUNDAL_PART  LIKE '%'
   AND A.ACTING_DATE  BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') AND (TO_DATE(:f_date||'1231','YYYYMMDD'))
   AND NVL(A.BANNAB_YN, 'N') = 'N'
   AND NVL(A.DC_YN, 'N') = 'N'
   AND A.NALSU &gt; 0
   AND B.HOSP_CODE = A.HOSP_CODE
   AND B.XRAY_CODE = A.HANGMOG_CODE
 GROUP BY TO_CHAR(A.ACTING_DATE, 'YYYY/MM')
 UNION ALL
SELECT '' ACTING_MONTH
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'CR', NVL(B.XRAY_REAL_CNT, 1)))), 0) CR_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'CR', NVL(B.XRAY_REAL_CNT, 1)))), 0) CR_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'DR', NVL(B.XRAY_REAL_CNT, 1)))), 0) DR_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'DR', NVL(B.XRAY_REAL_CNT, 1)))), 0) DR_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'CT', NVL(B.XRAY_REAL_CNT, 1)))), 0) CT_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'CT', NVL(B.XRAY_REAL_CNT, 1)))), 0) CT_CNT_Y
     , NVL(SUM(DECODE(B.JAERYO_YN, 'N', DECODE(A.JUNDAL_PART,'MRI', NVL(B.XRAY_REAL_CNT, 1)))), 0) MRI_CNT_N
     , NVL(SUM(DECODE(B.JAERYO_YN, 'Y', DECODE(A.JUNDAL_PART,'MRI', NVL(B.XRAY_REAL_CNT, 1)))), 0) MRI_CNT_Y
     , SUM(NVL(B.XRAY_REAL_CNT, 1)) TOTAL_CNT
     , '11'
  FROM XRT0001 B
     , OCS2003 A
 WHERE A.HOSP_CODE    = :f_hosp_code
   AND A.JUNDAL_TABLE = 'XRT'
   AND A.JUNDAL_PART  LIKE '%'
   AND A.ACTING_DATE  BETWEEN TO_DATE(:f_date||'0101','YYYYMMDD') AND (TO_DATE(:f_date||'1231','YYYYMMDD'))
   AND NVL(A.BANNAB_YN, 'N') = 'N'
   AND NVL(A.DC_YN, 'N') = 'N'
   AND A.NALSU &gt; 0
   AND B.HOSP_CODE = A.HOSP_CODE
   AND B.XRAY_CODE = A.HANGMOG_CODE
 GROUP BY TO_CHAR(A.ACTING_DATE, 'YYYY')
 ORDER BY 11, 1</value>
