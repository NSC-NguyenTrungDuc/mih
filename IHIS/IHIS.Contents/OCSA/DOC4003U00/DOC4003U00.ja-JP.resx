  <data name="grdDOC4003.QuerySQL" xml:space="preserve">
    <value>SELECT A.SYS_DATE        
     , A.SYS_ID          
     , A.UPD_DATE        
     , A.UPD_ID          
     , A.HOSP_CODE       
     , A.PKDOC4003       
     , A.SEQ             
     , TO_DATE(A.CREATE_DATE, 'YY/MM/DD') CREATE_DATE
     , A.TO_HOSPITAL_INFO
     , A.TO_SINRYOUKA    
     , A.TO_SINRYOUKA2    
     , A.TO_DOCTOR       
     , A.TO_DOCTOR2       
     , A.DOCTOR          
     , A.GWA             
     , A.SUNAME          
     , A.SEX             
     , A.ZIP             
     , A.ADDRESS         
     , A.TEL             
     , TO_DATE(A.BIRTH, 'YY/MM/DD') BIRTH        
     , A.JOB             
     , A.DISEASE         
     , A.CHECKUP_OPINION 
     , A.PRESCRIPTION    
     , A.BUNHO
     , SUBSTR(B.ZIP_CODE, 1, 3) || '-' || SUBSTR(B.ZIP_CODE, 4) ZIP_CODE
     , B.ADDRESS
     , 'TEL:' || B.TEL || ' FAX:' || B.FAX
     , B.YOYANG_NAME
  FROM DOC4003 A
     , BAS0001 B
 WHERE A.HOSP_CODE = :f_hosp_code
   AND A.BUNHO     = :f_bunho
   --
   AND B.HOSP_CODE = A.HOSP_CODE
   AND A.CREATE_DATE BETWEEN B.START_DATE
                         AND B.END_DATE
 ORDER BY A.SEQ DESC</value>
