  <data name="cboHoDong.UserSQL" xml:space="preserve">
    <value>SELECT '%'
     , FN_ADM_MSG(221)
  FROM DUAL
 UNION ALL
SELECT DISTINCT 
       A.HO_DONG
     , B.GWA_NAME 
  FROM DRG3010 A, BAS0260 B
 WHERE A.HOSP_CODE = FN_ADM_LOAD_HOSP_CODE()
   AND B.HOSP_CODE = A.HOSP_CODE
   AND A.HO_DONG = B.GWA
 ORDER BY 1</value>
