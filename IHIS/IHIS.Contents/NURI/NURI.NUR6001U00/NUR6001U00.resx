  <data name="grdNur6001.QuerySQL" xml:space="preserve">
    <value>SELECT BUNHO
     , TO_CHAR(FROM_DATE,'YYYY/MM/DD')
     , TO_CHAR(END_DATE,'YYYY/MM/DD')
     , TO_CHAR(FKINP1001)
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI1) CHK1
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI2) CHK2
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI3) CHK3
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI4) CHK4
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI5) CHK5
     , FN_NUR_LOAD_CODE_NAME('BEDSORE_BUWI', BEDSORE_BUWI6) CHK6
     , BEDSORE_BUWI1
     , BEDSORE_BUWI2
     , BEDSORE_BUWI3
     , BEDSORE_BUWI4
     , BEDSORE_BUWI5
     , BEDSORE_BUWI6
  FROM NUR6001 A
 WHERE A.HOSP_CODE = :f_hosp_code
   AND A.BUNHO     = :f_bunho
 ORDER BY A.BUNHO, A.FROM_DATE DESC</value>
  </data>
  <data name="grdImage.QuerySQL" xml:space="preserve">
    <value>SELECT :f_bunho                    BUNHO
     , :f_from_date                FROM_DATE
     , :f_buwi_gubun               BUWI_GUBUN
     , ASSESSOR_DATE               ASSESSOR_DATE
     , LTRIM(TO_CHAR(SEQ,'00000')) SEQ
     , '' IMAGE
     , ''
     , BEDSORE_BUWI_IMAGE IMAGE_PATH
  FROM NUR6004 A
 WHERE A.HOSP_CODE     = :f_hosp_code
   AND A.BUNHO         = :f_bunho
   AND A.FROM_DATE     = TO_DATE(:f_from_date, 'YYYY/MM/DD')
   AND A.BEDSORE_BUWI  LIKE :f_buwi_gubun
   AND A.ASSESSOR_DATE = TO_DATE(:f_assessor_date, 'YYYY/MM/DD')
 ORDER BY A.SEQ</value>
  </data>
