  <data name="dloPlanInfo.QuerySQL" xml:space="preserve">
    <value>SELECT  NVL(X.FKNUR4002, Y.FKNUR4002)             FKNUR4002,
        NVL(X.PKNUR4003, Y.PKNUR4003)             PKNUR4003,
        NVL(X.NUR_PLAN_OTE, Y.NUR_PLAN_OTE)       NUR_PLAN_OTE,
        NVL(X.NUR_PLAN, Y.NUR_PLAN)               NUR_PLAN,
        NVL(X.NUR_PLAN_NAME, Y.NUR_PLAN_NAME)     NUR_DIS_PLAN_NAME,
        --''                                        NUR_DIS_PLAN_NAME,
        NVL(X.NUR4003_VALD, Y.NUR4003_VALD)       NUR4003_VALD,
        NVL(X.PKNUR4004, Y.PKNUR4004)             PKNUR4004,
        NVL(X.NUR_PLAN_DETAIL, Y.NUR_PLAN_DETAIL) NUR_PLAN_DETAIL,
        NVL(X.NUR_PLAN_DENAME, Y.NUR_PLAN_DENAME) NUR_PLAN_DENAME,
        NVL(X.NUR4004_VALD, Y.NUR4004_VALD)       NUR4004_VALD,
        NVL(X.NUR_PLAN_NAME, Y.NUR_PLAN_NAME)     NUR_PLAN_NAME,
        NVL(X.NUR_SORT1, Y.NUR_SORT1)             NUR_SORT1,
        NVL(X.NUR_SORT2, Y.NUR_SORT2)             NUR_SORT2,
        NVL(X.NUR_SORT3, Y.NUR_SORT3)             NUR_SORT3
  FROM (SELECT  A.FKNUR4002                                FKNUR4002,
                A.PKNUR4003                                PKNUR4003,
                A.NUR_PLAN_OTE                             NUR_PLAN_OTE,
                A.NUR_PLAN                                 NUR_PLAN,
                A.NUR_PLAN_NAME                            NUR_PLAN_NAME,
                NVL(A.VALD, 'N')                           NUR4003_VALD,
                B.PKNUR4004                                PKNUR4004,
                NVL(B.NUR_PLAN_DETAIL, 'X')                NUR_PLAN_DETAIL,
                B.NUR_PLAN_DENAME                          NUR_PLAN_DENAME,
                NVL(B.VALD, 'N')                           NUR4004_VALD,
                DECODE(A.NUR_PLAN_OTE, 'P', '1'
                                     , 'O', '2'
                                     , 'T', '3'
                                     , '4' )               NUR_SORT1,
                LTRIM(TO_CHAR(NVL(A.SORT_KEY, 99), '00'))  NUR_SORT2,
                LTRIM(TO_CHAR(NVL(B.SORT_KEY, 99), '00'))  NUR_SORT3
          FROM  NUR4003 A,
                NUR4004 B
          WHERE A.HOSP_CODE     = :f_hosp_code
            AND B.HOSP_CODE(+)  = A.HOSP_CODE
            AND A.FKNUR4002     = :f_pknur4002            
            --AND A.VALD          = 'Y'
            --AND B.VALD(+)       = 'Y'
            AND B.FKNUR4003 (+) =  A.PKNUR4003
            AND ( A.NUR_PLAN      != '9999' OR
                  B.NUR_PLAN_DETAIL != 'XX' )  ) X,
        (SELECT TO_NUMBER(:f_pknur4002)                    FKNUR4002,
                0                                          PKNUR4003,
                A.NUR_PLAN_OTE                             NUR_PLAN_OTE,
                A.NUR_PLAN                                 NUR_PLAN,
                A.NUR_PLAN_NAME                            NUR_PLAN_NAME,
                'N'                                        NUR4003_VALD,
                0                                          PKNUR4004,
                NVL(B.NUR_PLAN_DETAIL, 'X')                NUR_PLAN_DETAIL,
                B.NUR_PLAN_DENAME                          NUR_PLAN_DENAME,
                'N'                                        NUR4004_VALD,
                DECODE(A.NUR_PLAN_OTE, 'P', '1'
                                     , 'O', '2'
                                     , 'T', '3'
                                     , '4' )               NUR_SORT1,
                LTRIM(TO_CHAR(NVL(A.SORT_KEY, 99), '00'))  NUR_SORT2,
                LTRIM(TO_CHAR(NVL(B.SORT_KEY, 99), '00'))  NUR_SORT3
           FROM NUR0403 A,
                NUR0404 B
          WHERE A.HOSP_CODE                                       = :f_hosp_code
            AND B.HOSP_CODE(+)                                    = A.HOSP_CODE
            AND A.NUR_PLAN_JIN                                    = :f_nur_plan_jin
            AND A.NUR_PLAN_PRO                                    = :f_nur_plan_pro
            AND A.FROM_DATE                                      &lt;= SYSDATE
            AND NVL(A.END_DATE, TO_DATE('99981231', 'YYYYMMDD')) &gt;= SYSDATE
            AND B.NUR_PLAN_JIN(+)                                 = A.NUR_PLAN_JIN
            AND B.NUR_PLAN_PRO(+)                                 = A.NUR_PLAN_PRO
            AND B.NUR_PLAN    (+)                                 = A.NUR_PLAN
            AND A.VALD          = 'Y'
            AND B.VALD(+)       = 'Y'                                    ) Y
 WHERE  X.NUR_PLAN_OTE(+) = Y.NUR_PLAN_OTE
   AND  X.NUR_PLAN(+) = Y.NUR_PLAN
   AND  X.NUR_PLAN_DETAIL(+) = Y.NUR_PLAN_DETAIL
 UNION 
SELECT  A.FKNUR4002                                FKNUR4002,
        A.PKNUR4003                                PKNUR4003,
        A.NUR_PLAN_OTE                             NUR_PLAN_OTE,
        A.NUR_PLAN                                 NUR_PLAN,
        A.NUR_PLAN_NAME                            NUR_PLAN_NAME,
        NVL(A.VALD, 'N')                           NUR4003_VALD,
        B.PKNUR4004                                PKNUR4004,
        NVL(B.NUR_PLAN_DETAIL, 'X')                NUR_PLAN_DETAIL,
        B.NUR_PLAN_DENAME                          NUR_PLAN_DENAME,
        NVL(B.VALD, 'N')                           NUR4004_VALD,
        A.NUR_PLAN_NAME                            NUR_PLAN_NAME,
        DECODE(A.NUR_PLAN_OTE, 'P', '1'
                             , 'O', '2'
                             , 'T', '3'
                             , '4' )               NUR_SORT1,
        LTRIM(TO_CHAR(NVL(A.SORT_KEY, 99), '00'))  NUR_SORT2,
        LTRIM(TO_CHAR(NVL(B.SORT_KEY, 99), '00'))  NUR_SORT3
  FROM  NUR4003 A,
        NUR4004 B
  WHERE A.HOSP_CODE     = :f_hosp_code
    AND B.HOSP_CODE(+)  = A.HOSP_CODE
    AND A.FKNUR4002     = :f_pknur4002         
    AND B.FKNUR4003 (+) =  A.PKNUR4003
    AND ( A.NUR_PLAN      = '9999' OR
          B.NUR_PLAN_DETAIL = 'XX' )
   
 ORDER BY 12,13,14
--          NVL(X.NUR_SORT1,Y.NUR_SORT1),
--          NVL(X.NUR_SORT2, Y.NUR_SORT2),
--          NVL(X.NUR_SORT3,Y.NUR_SORT3)</value>
