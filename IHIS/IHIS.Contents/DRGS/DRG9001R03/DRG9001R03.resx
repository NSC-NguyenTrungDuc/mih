  <data name="lay9001R.QuerySQL" xml:space="preserve">
    <value>SELECT E.HOLI_DAY                                      AS HOLI_DAY
     , NVL(A.SU, 0)                                    AS DRG_CNT
     , NVL(G.SU, 0)                                    AS DRG_GROUP_CNT
     , NVL(C.SU, 0)                                    AS DRG_HANGMOG_CNT
     , NVL(B.SU, 0)                                    AS INJ_CNT
     , NVL(H.SU, 0)                                    AS INJ_GROUP_CNT
     , NVL(D.SU, 0)                                    AS INJ_HANGMOG_CNT
     , NVL(A.SU, 0) + NVL(B.SU, 0)                     AS DRG_INJ_CNT
     , NVL(G.SU, 0) + NVL(H.SU, 0)                     AS DRG_INJ_GROUP_CNT
     , NVL(C.SU, 0) + NVL(D.SU, 0)                     AS DRG_INJ_HANGMOG_CNT
  FROM 
       -- 処方箋枚数 (DRG_BUNHO 単位を 処方箋1枚とする)
       (SELECT JUBSU_DATE , COUNT(*)  SU
          FROM (
                SELECT X.JUBSU_DATE JUBSU_DATE, X.DRG_BUNHO, SUM(1)
                  FROM DRG3010 X
                 WHERE X.HOSP_CODE  = :f_hosp_code
                   AND X.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND X.BUNRYU1    &lt;&gt; '4'
                   AND X.JUBSU_DATE IS NOT NULL
                   AND X.DRG_BUNHO  IS NOT NULL
                   AND X.HO_DONG    LIKE :f_ho_dong||'%'
                   AND NVL(X.DC_YN, 'N')     = 'N'
                   AND NVL(X.BANNAB_YN, 'N') = 'N' 
                   AND X.BUNHO               NOT LIKE '09%'
                 GROUP BY X.JUBSU_DATE, X.DRG_BUNHO
                )
         GROUP BY JUBSU_DATE ) A,
       -- 薬ＲＰ数 (オーダーのGROUP_SERではなくDRG2030のＲＰの件数)
       (SELECT JUBSU_DATE , COUNT(CNT)  SU
          FROM (
                SELECT X.JUBSU_DATE JUBSU_DATE, X.DRG_BUNHO, SUM(1) CNT
                  FROM DRG3011 X
                     , DRG3010 Y
                 WHERE X.HOSP_CODE  = :f_hosp_code
                   AND X.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND X.BUNRYU1    &lt;&gt; '4'
                   AND X.DRG_BUNHO  IS NOT NULL
                   AND X.HO_DONG    LIKE :f_ho_dong||'%'
                   AND X.BUNHO      NOT LIKE '09%'
                   --
                   AND Y.HOSP_CODE           = X.HOSP_CODE
                   AND Y.FKDRG3011           = X.PKDRG3011
                   AND NVL(Y.BANNAB_YN, 'N') = 'N'
                   AND NVL(Y.DC_YN, 'N')     = 'N'
                 GROUP BY X.JUBSU_DATE, X.DRG_BUNHO, X.GROUP_SER
               )
         GROUP BY JUBSU_DATE ) G,
       -- 薬件数 (薬局に伝達されたオーダーの件数)
       (SELECT JUBSU_DATE , SUM(CNT)  SU
          FROM (
                SELECT X.JUBSU_DATE JUBSU_DATE, X.DRG_BUNHO, COUNT(X.JAERYO_CODE) CNT--, X.GROUP_SER, SUM(1)
                  FROM DRG3011 X
                     , DRG3010 Y
                 WHERE X.HOSP_CODE  = :f_hosp_code
                   AND X.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND X.BUNRYU1   &lt;&gt; '4'
                   AND X.DRG_BUNHO IS NOT NULL
                   AND X.HO_DONG   LIKE :f_ho_dong||'%'
                   AND X.BUNHO     NOT LIKE '09%'
                   --
                   AND Y.HOSP_CODE = X.HOSP_CODE
                   AND Y.FKDRG3011 = X.PKDRG3011
                   AND NVL(Y.BANNAB_YN, 'N') = 'N'
                   AND NVL(Y.DC_YN, 'N')     = 'N'
                 GROUP BY X.JUBSU_DATE, X.DRG_BUNHO
               )
         GROUP BY JUBSU_DATE) C,
       -- 注射箋枚数
       (SELECT JUBSU_DATE , COUNT(*)  SU
          FROM (
                SELECT Y.JUBSU_DATE JUBSU_DATE, Y.DRG_BUNHO, SUM(1)
                  FROM DRG3010 Y
                 WHERE Y.HOSP_CODE = :f_hosp_code
                   AND Y.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND Y.BUNRYU1   = '4'
                   AND Y.DRG_BUNHO IS NOT NULL
                   AND Y.HO_DONG   LIKE :f_ho_dong||'%'
                   AND Y.BUNHO     NOT LIKE '09%'
                   AND NVL(Y.DC_YN, 'N')     = 'N'
                   AND NVL(Y.BANNAB_YN, 'N') = 'N' 
                 GROUP BY Y.JUBSU_DATE, Y.DRG_BUNHO
                 )
         GROUP BY JUBSU_DATE ) B,
       -- 注射ＲＰ数
       (SELECT JUBSU_DATE , COUNT(CNT)  SU
          FROM (
                SELECT Y.JUBSU_DATE JUBSU_DATE, Y.DRG_BUNHO, SUM(1) CNT
                  FROM DRG3011 Y
                     , DRG3010 X
                 WHERE Y.HOSP_CODE  = :f_hosp_code
                   AND Y.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND Y.BUNRYU1   = '4'
                   AND Y.DRG_BUNHO IS NOT NULL
                   AND Y.HO_DONG   LIKE :f_ho_dong||'%'
                   AND Y.BUNHO     NOT LIKE '09%'
                   --
                   AND X.HOSP_CODE = Y.HOSP_CODE
                   AND X.FKDRG3011 = Y.PKDRG3011
                   AND NVL(X.BANNAB_YN, 'N') = 'N'
                   AND NVL(X.DC_YN, 'N')     = 'N'
                 GROUP BY Y.JUBSU_DATE, Y.DRG_BUNHO, Y.GROUP_SER
               )
         GROUP BY JUBSU_DATE ) H,
       -- 注射件数
       (SELECT JUBSU_DATE , SUM(CNT)  SU
          FROM (
                SELECT Y.JUBSU_DATE JUBSU_DATE, Y.DRG_BUNHO, COUNT(Y.JAERYO_CODE) CNT--, Y.GROUP_SER
                  FROM DRG3011 Y
                     , DRG3010 X
                 WHERE Y.HOSP_CODE = :f_hosp_code
                   AND Y.JUBSU_DATE BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') 
                                        AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
                   AND Y.BUNRYU1   = '4'
                   AND Y.DRG_BUNHO IS NOT NULL
                   AND Y.HO_DONG   LIKE :f_ho_dong||'%'
                   AND Y.BUNHO     NOT LIKE '09%'
                   --
                   AND X.HOSP_CODE = Y.HOSP_CODE
                   AND X.FKDRG3011 = Y.PKDRG3011
                   AND NVL(X.BANNAB_YN, 'N') = 'N'
                   AND NVL(X.DC_YN, 'N')     = 'N'
                 GROUP BY Y.JUBSU_DATE, Y.DRG_BUNHO
               )
         GROUP BY JUBSU_DATE ) D,
                        RES0101 E
 WHERE E.HOSP_CODE  = :f_hosp_code
   AND E.HOLI_DAY = A.JUBSU_DATE(+)
   AND E.HOLI_DAY = B.JUBSU_DATE(+)
   AND E.HOLI_DAY = C.JUBSU_DATE(+)
   AND E.HOLI_DAY = D.JUBSU_DATE(+)
   AND E.HOLI_DAY = G.JUBSU_DATE(+)
   AND E.HOLI_DAY = H.JUBSU_DATE(+)
   AND E.HOLI_DAY BETWEEN TO_DATE(:f_date||'01','YYYYMMDD') AND LAST_DAY(TO_DATE(:f_date,'YYYYMM'))
 ORDER BY E.HOLI_DAY</value>
