  <data name="laySearchTree.QuerySQL" xml:space="preserve">
    <value>SELECT A.BUSEO_NAME
     , 'Y'
     , A.BUSEO_CODE
     , NULL
     , A.PARENT_BUSEO
     , FN_BAS_LOAD_BUSEO_NAME(A.PARENT_BUSEO,SYSDATE) 
  FROM BAS0260 A
 WHERE A.HOSP_CODE                = :f_hosp_code
   AND (   A.BUSEO_NAME           LIKE '%'||:f_search_text||'%'
        OR A.BUSEO_CODE           LIKE '%'||:f_search_text||'%'
       )
   AND A.START_DATE               = (SELECT MAX(Z.START_DATE)
                                       FROM BAS0260          Z
                                      WHERE Z.HOSP_CODE      = A.HOSP_CODE
                                        AND Z.BUSEO_CODE     = A.BUSEO_CODE
                                        AND (   Z.END_DATE   IS NULL
                                             OR Z.END_DATE   &gt;= TRUNC(SYSDATE)
                                            )
                                    )
   AND (    A.END_DATE   IS NULL
         OR A.END_DATE   &gt;= TRUNC(SYSDATE)
       )
   AND EXISTS                     (
                                    SELECT NULL
                                      FROM ADM3200              Z
                                     WHERE Z.HOSP_CODE          = A.HOSP_CODE
                                       AND Z.DEPT_CODE          = A.BUSEO_CODE
                                       AND (   Z.USER_END_YMD   IS NULL
                                            OR Z.USER_END_YMD   &gt;= TRUNC(SYSDATE)
                                           )
                                  )
UNION ALL
SELECT A.USER_NM
     , 'N'
     , NULL
     , A.USER_ID
     , A.DEPT_CODE
     , FN_BAS_LOAD_BUSEO_NAME(A.DEPT_CODE, SYSDATE)
  FROM ADM3200 A
 WHERE A.HOSP_CODE          = :f_hosp_code
   AND (   A.USER_NM        LIKE '%'||:f_search_text||'%'
        OR A.USER_ID        LIKE '%'||:f_search_text||'%'
       )
   AND (   A.USER_END_YMD   IS NULL
        OR A.USER_END_YMD   &gt;= TRUNC(SYSDATE)
       )
</value>
  </data>
